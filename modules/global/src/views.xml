<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="ch.shire.bandman.entity.Inventory"
          name="inventoryWithAssets">
        <property name="name"/>
        <property name="assets">
            <property name="name"/>
            <property name="quantity"/>
            <property name="price"/>
        </property>
    </view>
    <view class="ch.shire.bandman.entity.Asset"
          name="assetWithOwners">
        <property name="name"/>
        <property name="quantity"/>
        <property name="price"/>
        <property name="owners">
            <property name="name"/>
            <property name="firstName"/>
            <property name="lastName"/>
            <property name="email"/>
        </property>
    </view>
</views>
